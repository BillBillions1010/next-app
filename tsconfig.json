{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts", 
    "**/*.ts", 
    "**/*.tsx", 
    ".next/types/**/*.ts",
    "app/lib/placeholder-data.js",
    "scripts/seed.js"
, "app/pages/about.jsx", "app/pages/_App.jsxapp/pages/_document.jsx.jsx", "pages/componentsapp/pages/index.tsxndex.tsx", "app/pages/landing.jsx", "app/pages/login.jsx", "app/pages/profile.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
